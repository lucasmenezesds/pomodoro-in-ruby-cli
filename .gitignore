# bundle gem
/.yardoc
/_yardoc/
/coverage/
/doc/
/pkg/
/tmp/

# Ignore bundler config.
/.bundle/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-*

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
.byebug_history

# rspec failure tracking and reports
.rspec_status
/spec/reports/

# Ignore master key for decrypting credentials and more.
/config/master.key
/.env
/.idea

.env.*